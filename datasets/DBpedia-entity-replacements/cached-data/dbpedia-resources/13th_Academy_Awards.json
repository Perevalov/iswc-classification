        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "13th Academy Awards",
                "lang": "en"
            },
            {
                "type": "literal",
                "value": "13e c\u00e9r\u00e9monie des Oscars",
                "lang": "fr"
            },
            {
                "type": "literal",
                "value": "13. ceremonia wr\u0119czenia Oscar\u00f3w",
                "lang": "pl"
            },
            {
                "type": "literal",
                "value": "\u062d\u0641\u0644 \u062a\u0648\u0632\u064a\u0639 \u062c\u0648\u0627\u0626\u0632 \u0627\u0644\u0623\u0648\u0633\u0643\u0627\u0631 \u0627\u0644\u062b\u0627\u0644\u062b \u0639\u0634\u0631",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "\u7b2c13\u56de\u30a2\u30ab\u30c7\u30df\u30fc\u8cde",
                "lang": "ja"
            },
            {
                "type": "literal",
                "value": "Oscarverleihung 1941",
                "lang": "de"
            },
            {
                "type": "literal",
                "value": "Oscar 1941",
                "lang": "pt"
            },
            {
                "type": "literal",
                "value": "\u041e\u0441\u043a\u0430\u0440 (\u043a\u0438\u043d\u043e\u043f\u0440\u0435\u043c\u0438\u044f, 1941)",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "Premi Oscar 1941",
                "lang": "it"
            },
            {
                "type": "literal",
                "value": "13de Oscaruitreiking",
                "lang": "nl"
            },
            {
