            },
            {
                "type": "literal",
                "value": "\u5358\u4f4d\u306e\u63db\u7b97\u4e00\u89a7",
                "lang": "ja"
            },
            {
                "type": "literal",
                "value": "Conversi\u00f3n de unidades",
