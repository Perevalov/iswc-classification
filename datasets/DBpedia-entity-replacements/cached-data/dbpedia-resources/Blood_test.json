        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "\u8840\u6db2\u691c\u67fb",
                "lang": "ja"
            },
            {
                "type": "literal",
                "value": "Blood test",
                "lang": "en"
            },
            {
                "type": "literal",
                "value": "Examen de sangre",
                "lang": "es"
            },
            {
                "type": "literal",
                "value": "\u8840\u6db2\u68c0\u67e5",
                "lang": "zh"
            },
            {
                "type": "literal",
                "value": "\u062a\u062d\u0644\u064a\u0644 \u0627\u0644\u062f\u0645",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "Esami del sangue",
                "lang": "it"
            },
            {
                "type": "literal",
                "value": "Exame de sangue",
                "lang": "pt"
            },
            {
