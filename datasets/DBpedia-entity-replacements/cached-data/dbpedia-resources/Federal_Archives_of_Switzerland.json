        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "Federal Archives of Switzerland",
                "lang": "en"
            },
            {
                "type": "literal",
                "value": "Schweizerisches Bundesarchiv",
                "lang": "de"
