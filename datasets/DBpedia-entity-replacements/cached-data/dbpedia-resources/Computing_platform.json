                "lang": "fr"
            },
            {
                "type": "literal",
                "value": "Platform (informatica)",
                "lang": "nl"
            },
            {
                "type": "literal",
                "value": "Plataforma (inform\u00e1tica)",
                "lang": "es"
            },
            {
                "type": "literal",
                "value": "\u0645\u0646\u0635\u0629 \u062d\u0627\u0633\u0648\u0628",
