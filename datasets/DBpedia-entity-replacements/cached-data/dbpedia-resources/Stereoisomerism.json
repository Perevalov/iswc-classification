            {
                "type": "literal",
                "value": "\u0421\u0442\u0435\u0440\u0435\u043e\u0438\u0437\u043e\u043c\u0435\u0440\u044b",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "\u7acb\u4f53\u7570\u6027\u4f53",
