        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "\u0637\u0627\u0642\u0629 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "\u042d\u043d\u0435\u0440\u0433\u0438\u044f \u0441\u0432\u044f\u0437\u0438",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "\u7d50\u5408\u30a8\u30cd\u30eb\u30ae\u30fc",
                "lang": "ja"
            },
            {
