            },
            {
                "type": "literal",
                "value": "\u30ed\u30c9\u30f4\u30a3\u30b32\u4e16",
                "lang": "ja"
            },
            {
                "type": "literal",
                "value": "Louis II d'Italie",
                "lang": "fr"
            },
            {
                "type": "literal",
                "value": "Lu\u00eds II da Germ\u00e2nia",
                "lang": "pt"
            },
            {
                "type": "literal",
                "value": "\u0644\u0648\u064a\u0633 \u0627\u0644\u062b\u0627\u0646\u064a \u0645\u0644\u0643 \u0625\u064a\u0637\u0627\u0644\u064a\u0627",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "Ludwik II (cesarz)",
                "lang": "pl"
            },
            {
                "type": "literal",
                "value": "\u8def\u6613\u4e8c\u4e16 (\u610f\u5927\u5229)",
                "lang": "zh"
            },
            {
                "type": "literal",
                "value": "Louis II of Italy",
                "lang": "en"
            },
            {
                "type": "literal",
                "value": "Ludwig II. (Italien)",
                "lang": "de"
            },
            {
                "type": "literal",
                "value": "Ludovico II il Giovane",
                "lang": "it"
            },
            {
                "type": "literal",
                "value": "\u041b\u044e\u0434\u043e\u0432\u0438\u043a II (\u043a\u043e\u0440\u043e\u043b\u044c \u0418\u0442\u0430\u043b\u0438\u0438)",
                "lang": "ru"
            },
            {
