        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "Internowanie",
                "lang": "pl"
            },
            {
                "type": "literal",
                "value": "\u0418\u043d\u0442\u0435\u0440\u043d\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "Internierung",
                "lang": "de"
            },
            {
                "type": "literal",
                "value": "Internment",
                "lang": "en"
            },
            {
                "type": "literal",
                "value": "Internement",
                "lang": "fr"
