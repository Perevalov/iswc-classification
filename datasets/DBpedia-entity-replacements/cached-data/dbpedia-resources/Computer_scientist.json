            {
                "type": "literal",
                "value": "\u0639\u0627\u0644\u0645 \u062d\u0627\u0633\u0648\u0628",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "Computer scientist",
                "lang": "en"
