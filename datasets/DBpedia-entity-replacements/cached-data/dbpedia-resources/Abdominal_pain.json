                "lang": "ja"
            },
            {
                "type": "literal",
                "value": "B\u00f3l brzucha",
                "lang": "pl"
            },
            {
                "type": "literal",
                "value": "Abdominal pain",
                "lang": "en"
            },
            {
                "type": "literal",
                "value": "Dolor abdominal",
                "lang": "es"
            },
            {
                "type": "literal",
                "value": "\u0411\u043e\u043b\u0438 \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0436\u0438\u0432\u043e\u0442\u0430 \u0438 \u0442\u0430\u0437\u0430",
                "lang": "ru"
            },
