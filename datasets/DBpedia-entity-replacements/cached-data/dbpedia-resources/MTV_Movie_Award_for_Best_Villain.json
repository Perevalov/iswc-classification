        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "Pr\u00e9mio MTV Movie de melhor vil\u00e3o",
                "lang": "pt"
            },
            {
                "type": "literal",
                "value": "MTV Movie Award al miglior cattivo",
                "lang": "it"
            },
            {
                "type": "literal",
                "value": "\u062c\u0627\u0626\u0632\u0629 \u0625\u0645 \u062a\u064a \u0641\u064a \u0644\u0623\u0641\u0636\u0644 \u0634\u0631\u064a\u0631",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "\u041a\u0438\u043d\u043e\u043d\u0430\u0433\u0440\u0430\u0434\u0430 MTV \u043b\u0443\u0447\u0448\u0435\u043c\u0443 \u043a\u0438\u043d\u043e\u0437\u043b\u043e\u0434\u0435\u044e",
                "lang": "ru"
            },
            {
                "type": "literal",
