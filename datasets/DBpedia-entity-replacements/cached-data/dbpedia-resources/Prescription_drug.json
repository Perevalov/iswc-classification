            },
            {
                "type": "literal",
                "value": "\u51e6\u65b9\u7b8b\u533b\u85ac\u54c1",
                "lang": "ja"
            },
            {
                "type": "literal",
                "value": "\u062f\u0648\u0627\u0621 \u0627\u0644\u0648\u0635\u0641\u0627\u062a",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "Prescription drug",
