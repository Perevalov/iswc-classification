        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "\u0411\u0435\u0433\u043e\u0432\u0430\u044f \u0434\u043e\u0440\u043e\u0436\u043a\u0430 (\u0441\u043f\u043e\u0440\u0442\u0438\u0432\u043d\u044b\u0439 \u0442\u0440\u0435\u043d\u0430\u0436\u0451\u0440)",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "\u062c\u0647\u0627\u0632 \u0627\u0644\u0645\u0634\u064a",
                "lang": "ar"
            },
            {
