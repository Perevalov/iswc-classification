                "lang": "es"
            },
            {
                "type": "literal",
                "value": "\u0427\u0440\u0435\u0432\u043e\u0432\u0435\u0449\u0430\u043d\u0438\u0435",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "Buikspreken",
                "lang": "nl"
            },
            {
                "type": "literal",
