            {
                "type": "literal",
                "value": "Educa\u00e7\u00e3o obrigat\u00f3ria",
                "lang": "pt"
            },
            {
                "type": "literal",
                "value": "\u041e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u0435",
