            {
                "type": "literal",
                "value": "\u041f\u0440\u0438\u0442\u0447\u0430 \u043e \u0434\u043e\u0431\u0440\u043e\u043c \u0441\u0430\u043c\u0430\u0440\u044f\u043d\u0438\u043d\u0435",
                "lang": "ru"
            },
            {
