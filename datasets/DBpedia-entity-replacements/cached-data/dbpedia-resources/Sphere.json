        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "Bol (lichaam)",
                "lang": "nl"
            },
            {
                "type": "literal",
                "value": "Esfera",
                "lang": "pt"
            },
            {
                "type": "literal",
                "value": "\u0421\u0444\u0435\u0440\u0430",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "\u0643\u0631\u0629",
                "lang": "ar"
            },
            {
                "type": "literal",
                "value": "Sphere",
                "lang": "en"
            },
            {
                "type": "literal",
