        "http://www.w3.org/2000/01/rdf-schema#label": [
            {
                "type": "literal",
                "value": "\u041a\u0443\u0440\u0441\u0438\u0432",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "Italique (typographie)",
                "lang": "fr"
            },
            {
                "type": "literal",
                "value": "\u062e\u0637 \u0627\u0644\u0637\u0628\u0627\u0639\u0629 \u0627\u0644\u0645\u0627\u0626\u0644",
                "lang": "ar"
            },
            {
                "type": "literal",
