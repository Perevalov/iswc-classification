            },
            {
                "type": "literal",
                "value": "\u041e\u0441\u043a\u0430\u0440 (\u043a\u0438\u043d\u043e\u043f\u0440\u0435\u043c\u0438\u044f, 1960)",
                "lang": "ru"
            },
            {
