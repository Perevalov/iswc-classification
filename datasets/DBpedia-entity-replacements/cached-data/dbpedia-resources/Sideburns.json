            },
            {
                "type": "literal",
                "value": "\u0411\u0430\u043a\u0435\u043d\u0431\u0430\u0440\u0434\u044b",
                "lang": "ru"
            },
            {
                "type": "literal",
                "value": "\u0633\u0627\u0644\u0641 \u0627\u0644\u0634\u0639\u0631",
